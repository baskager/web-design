<li class="thumbnail">
    <img alt='' src='{{img}}'>
    <a href='{{url}}' class='image-wrap'>
        <div class='caption'>
            <h3 id="title">{{title}}</h3>
            <p id="description">
                {{description}}
            </p>
            <ul id="tags" class='tags'>
                <li>Javascript</li>
                <li>Facial recognition</li>
                <li>API</li>
                <li class='year'>2018</li>
            </ul>
        </div>
    </a>
</li>